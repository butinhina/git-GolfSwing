<div class="mt-5 ml-5">
  <%= form_with url: public_post_videos_path, method: :get do |f| %>
    <%= text_field_tag :tag %>
    <%= submit_tag 'タグを追加' %>
  <% end %>
</div>
<div class="container mt-3">
  <div class="row">
    <div class="col-2 bg-light">
      <%= render 'list' %>
    </div>
    <div class="col-10">
      <%= render 'index', post_videos: @post_videos %>
    </div>
  </div>
</div>
